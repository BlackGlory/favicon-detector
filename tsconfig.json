{
  "compilerOptions": {
    "target": "ES2018"
  , "module": "ES2015"
  , "moduleResolution": "Node"
  , "jsx": "react-jsx"
  , "removeComments": true
  , "strict": true
  , "noUnusedParameters": false
  , "esModuleInterop": true
  , "baseUrl": "."
  , "paths": {
      "@src/*": ["src/*"],
      "@utils/*": ["src/utils/*"],
      "@components/*": ["src/components/*"]
    }
  }
, "include": [
    "src"
  ]
}
